<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.infoparks.wapi.infoparks">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Nairobi" />
        <activity android:name=".Central" />
        <activity android:name=".Eastern" />
        <activity android:name=".NorthEastern" />
        <activity android:name=".Coast" />
        <activity android:name=".RiftValley" />
        <activity android:name=".Nyanza" />
        <activity android:name=".Western" />
        <activity android:name=".Splash">

            <intent-filter>

                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
    </application>

</manifest>
